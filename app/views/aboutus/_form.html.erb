<%= form_for(@aboutu) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <div class="field">
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </div>
  <div class="actions">
    <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
  </div>
<% end %>
