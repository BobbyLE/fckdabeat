<%= tinymce %>
<%= form_for(@aboutu) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <div class="field">
    <%= f.label :content %><br />
    <% #f.text_area :content %>
    <%= f.text_area :content, :class => "tinymce", :rows => 20, :cols => 120 %>
  </div>
  <div class="actions">
    <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
  </div>
<% end %>
