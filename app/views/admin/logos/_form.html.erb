<%= form_for [:admin, @logo],  :html => { :multipart => true } do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">  	
  	<%= f.label :logofile %>
  	<%= f.file_field :logofile %>
  </div>
  <%= f.submit "Upload logo", class: "btn btn-large btn-primary" %>
<% end %>